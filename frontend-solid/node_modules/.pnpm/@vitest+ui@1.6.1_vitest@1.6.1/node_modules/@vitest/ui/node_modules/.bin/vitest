#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/André/Documents/Agent_Solution_BI/frontend-solid/node_modules/.pnpm/vitest@1.6.1_@types+node@20_66e4b557fbc9320fd831fa1b481e9584/node_modules/vitest/node_modules:/mnt/c/Users/André/Documents/Agent_Solution_BI/frontend-solid/node_modules/.pnpm/vitest@1.6.1_@types+node@20_66e4b557fbc9320fd831fa1b481e9584/node_modules:/mnt/c/Users/André/Documents/Agent_Solution_BI/frontend-solid/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/André/Documents/Agent_Solution_BI/frontend-solid/node_modules/.pnpm/vitest@1.6.1_@types+node@20_66e4b557fbc9320fd831fa1b481e9584/node_modules/vitest/node_modules:/mnt/c/Users/André/Documents/Agent_Solution_BI/frontend-solid/node_modules/.pnpm/vitest@1.6.1_@types+node@20_66e4b557fbc9320fd831fa1b481e9584/node_modules:/mnt/c/Users/André/Documents/Agent_Solution_BI/frontend-solid/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vitest@1.6.1_@types+node@20_66e4b557fbc9320fd831fa1b481e9584/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vitest@1.6.1_@types+node@20_66e4b557fbc9320fd831fa1b481e9584/node_modules/vitest/vitest.mjs" "$@"
fi
